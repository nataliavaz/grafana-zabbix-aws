                <item>
                    <name>Alarms - ALARM</name>
                    <key>alarm.aws[{$PROFILE},{$REGION}]</key>
                    <delay>3h</delay>
                    <applications>
                        <application>
                            <name>CloudWatch</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>AMIs</name>
                    <key>ami.aws[{$PROFILE},{$REGION},{$ACCOUNT}]</key>
                    <delay>3h</delay>
                    <applications>
                        <application>
                            <name>EC2</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Volumes Available</name>
                    <key>avolumes.prd[{$PROFILE},{$REGION}]</key>
                    <delay>3h</delay>
                    <applications>
                        <application>
                            <name>EC2</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Backup Failed</name>
                    <type>EXTERNAL</type>
                    <key>aws-backup-failed.sh[{$PROFILE}]</key>
