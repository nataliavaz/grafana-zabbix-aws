                    <name>Backup</name>
                </application>
                <application>
                    <name>Billing</name>
                </application>
                <application>
                    <name>CloudWatch</name>
                </application>
                <application>
                    <name>CodeDeploy</name>
                </application>
                <application>
                    <name>EC2</name>
                </application>
                <application>
                    <name>ECS</name>
                </application>
                <application>
                    <name>ElastiCache</name>
                </application>
                <application>
                    <name>IAM</name>
                </application>
                <application>
                    <name>Lambda</name>
                </application>
                <application>
                    <name>RDS</name>
                </application>
